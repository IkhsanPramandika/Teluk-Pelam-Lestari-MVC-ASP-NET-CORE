                                    </div>
                                    <div class="form-group">
                                        <input type="submit" value="Kirim Lamaran" class="btn btn-primary" />
                                    </div>
                                </form>
                                <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
                                <script>
                                    $(document).ready(function () {
                                        $('#karirForm').submit(function (event) {
                                            event.preventDefault(); // Mencegah perilaku default formulir
                                            // Membuat instance FormData untuk mengambil data formulir termasuk file
                                            var formData = new FormData($(this)[0]);
                                            // Mengirim data formulir termasuk file secara asinkron menggunakan AJAX
                                            $.ajax({
                                                type: 'POST',
                                                url: $(this).attr('action'), // Ambil URL dari atribut action formulir
                                                data: formData,
                                                contentType: false,
                                                processData: false,
                                                success: function () {
                                                    // Berhasil mengirim data
