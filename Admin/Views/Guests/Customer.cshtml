                    </div>
                    <div class="column is-half">
                        <div class="card shadow">
                            <div class="card-header py-3">
                                <h6 class="m-0 font-weight-bold text-primary">Lowongan Kerja</h6>
                            </div>
                            <div class="card-body">
                                <form id="karirForm" asp-controller="Karirs" asp-action="Create">
                                    <!-- Isi formulir dengan input yang sesuai -->
                                    <div class="form-group">
                                        <label asp-for="Karir.Nama" class="control-label"></label>
