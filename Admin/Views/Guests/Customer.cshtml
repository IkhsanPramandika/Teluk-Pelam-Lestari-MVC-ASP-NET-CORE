                                        <span asp-validation-for="Karir.email" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="Karir.Jabatan" class="control-label"></label>
                                        <select asp-for="Karir.Jabatan" class="form-control" required>
                                            <option value="">Pilih Jabatan</option>
