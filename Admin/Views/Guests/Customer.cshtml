    <section class="hero is-link" id="produk">
        <div class="hero-body">
            <div class="container">
                <h1 class="title has-text-centered">Yuk cobain udang vaname dari tambak kami </h1>
                <p class="subtitle has-text-centered">Udang Vaname dari Teluk Pelam Lestari merupakan udang dengan kualitas internasional</p>
                <div class="columns is-vcentered">
                    <div class="column is-half">
                        <div class="card-image">
                            <figure class="has-text-centered">
                                <img src="~/img/price.png" alt="Placeholder image" />
                            </figure>
                        </div>
                    </div>
                    <div class="column is-half">
                        <div class="card shadow">
                            <div class="card-header py-3">
                                <h6 class="m-0 font-weight-bold text-primary">Beli Udang Vaname</h6>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <form id="myForm" asp-controller="Transaksis" asp-action="Create" method="post">
                                        <!-- Isi formulir dengan input yang sesuai -->
                                        <div class="form-group">
                                            <label asp-for="Transaksi.NamaPembeli" class="control-label">Nama Pembeli :</label>
                                            <input asp-for="Transaksi.NamaPembeli" class="form-control" />
                                            <span asp-validation-for="Transaksi.NamaPembeli" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Transaksi.Status" class="control-label">Jenis Udang :</label>
                                            <select asp-for="Transaksi.Status" class="form-control" id="Status">
                                                <option value="Udang Kecil">Udang Kecil</option>
                                                <option value="Udang Sedang">Udang Sedang</option>
                                                <option value="Udang Besar">Udang Besar</option>
                                            </select>
                                            <span asp-validation-for="Transaksi.Status" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Transaksi.TotalBeli" class="control-label">Total Beli :</label>
                                            <input asp-for="Transaksi.TotalBeli" class="form-control" id="TotalBeli" />
                                            <span asp-validation-for="Transaksi.TotalBeli" class="text-danger"></span>
                                        </div>
                                        <div class="form-group total-harga">
                                            <label asp-for="Transaksi.TotalHarga" class="control-label">Total Harga :</label>
                                            <input asp-for="Transaksi.TotalHarga" class="form-control" id="TotalHarga" readonly value="0" />
                                            <span asp-validation-for="Transaksi.TotalHarga" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Transaksi.ReleaseDate" class="control-label">Tanggal :</label>
                                            <input asp-for="Transaksi.ReleaseDate" class="form-control" type="date" id="ReleaseDate" readonly value="<%= DateTime.Now.ToString(" yyyy-MM-dd") % />
                                            <span asp-validation-for="Transaksi.ReleaseDate" class="text-danger"></span>
                                        </div>
                                        <!-- Tombol Submit -->
                                        <div class="form-group">
                                            <input type="submit" value="Pesan Udang" class="btn btn-primary" />
                                        </div>
                                    </form>

                                    <script>
                                        document.addEventListener('DOMContentLoaded', function () {
                                            const releaseDateInput = document.getElementById('ReleaseDate');
                                            const today = new Date().toISOString().split('T')[0];
                                            releaseDateInput.setAttribute('value', today); // Menetapkan nilai tanggal ke hari ini pada saat halaman dimuat

                                            const jenisUdang = document.getElementById('Status');
                                            const totalBeliInput = document.getElementById('TotalBeli');
                                            const totalHargaInput = document.getElementById('TotalHarga');

                                            jenisUdang.addEventListener('change', function () {
                                                calculateTotal();
                                            });

                                            totalBeliInput.addEventListener('input', function () {
                                                calculateTotal();
                                            });

                                            function calculateTotal() {
                                                const jumlahBeli = parseInt(totalBeliInput.value) || 0;
                                                let harga = 0;

                                                switch (jenisUdang.value) {
                                                    case 'Udang Kecil':
                                                        harga = 10000;
                                                        break;
                                                    case 'Udang Sedang':
                                                        harga = 15000;
                                                        break;
                                                    case 'Udang Besar':
                                                        harga = 20000;
                                                        break;
                                                    default:
                                                        harga = 0;
                                                        break;
                                                }

                                                const total = jumlahBeli * harga;
                                                totalHargaInput.value = total.toString(); // Konversi ke string sebelum ditampilkan
                                            }

                                            calculateTotal(); // Hitung total saat halaman dimuat
                                        });
                                    </script>
                                    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
                                    <script>
                                        $(document).ready(function () {
                                            $('#myForm').submit(function (event) {
                                                event.preventDefault(); // Mencegah perilaku default formulir

                                                // Ambil data formulir
                                                var formData = $(this).serialize();

                                                // Kirim data formulir secara asinkron menggunakan AJAX
                                                $.ajax({
                                                    type: 'POST',
                                                    url: $(this).attr('action'), // Ambil URL dari atribut action formulir
                                                    data: formData,
                                                    success: function (response) {
                                                        // Berhasil mengirim data
                                                        // Lakukan tindakan yang diperlukan (misalnya, tampilkan pesan sukses)
                                                        alert('Pesanan anda berhasil dikirim!');

                                                        // Di sini Anda dapat memperbarui halaman jika diperlukan
                                                        // Contoh: memuat ulang bagian tertentu dari halaman
                                                        // $('#targetElement').html(response);
                                                    },
                                                    error: function () {
                                                        // Terjadi kesalahan saat mengirim data
                                                        // Lakukan penanganan kesalahan jika diperlukan
                                                        alert('Terjadi kesalahan saat mengirim data.');
                                                    }
                                                });
                                            });
                                        });
                                    </script>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
    </section>


