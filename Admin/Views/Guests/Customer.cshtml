    <!-- Lamaran Karir-->
    <section class="hero is-link" id="karir">
        <div class="hero-body">
            <div class="container">
                <h1 class="title has-text-centered">Bergabunglah Bersama Kami!</h1>
                <p class="subtitle has-text-centered">Teluk Pelam Lestari membuka kesempatan bagi individu yang bersemangat untuk bergabung dengan tim kami.</p>
                <div class="columns is-vcentered">
                    <div class="column is-half">
                        <div class="card-image">
                            <figure class="has-text-centered">
                                <img src="~/img/loker1.png" alt="Placeholder image">
                            </figure>
